<%= CSV.generate_line(['name', 'manufacturer', 'model', 'case', 'serial_number', 'price']) -%>
<%- @items.each do |i| -%>
<%= CSV.generate_line([
        i.name,
        i.manufacturer,
        i.model,
        i.case.acronym,
        i.serial_number,
        i.price.nil? ? '' : "#{'%.2f' % i.price}",
    ]).html_safe -%>
<%- end -%>
